                  <TextField fx:id="addNumberInput" />
               </children>
            </VBox>
            <VBox prefHeight="29.0" prefWidth="200.0">
               <children>
                  <Label text="Title:" textFill="WHITE">
                     <font>
                        <Font name="Elephant" size="12.0" />
                     </font></Label>
