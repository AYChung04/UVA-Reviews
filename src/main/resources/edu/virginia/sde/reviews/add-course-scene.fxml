<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="100.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.reviews.AddCourseController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#232D4B" height="108.0" layoutX="-1.0" stroke="#F84C1E" strokeType="INSIDE" strokeWidth="5.0" width="406.0" />
      <Button fx:id="addButton" layoutX="353.0" layoutY="68.0" mnemonicParsing="false" onAction="#addCourseSearch" text="Add" />
