<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="350.0" style="-fx-background-color: #232D4B;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.reviews.CreateUserController">
   <children>
      <VBox layoutX="71.0" layoutY="202.0" prefHeight="200.0" prefWidth="208.0">
         <children>
            <Label text="Username (required):" textFill="WHITE">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font></Label>
            <TextField fx:id="createUsernameInput" />
            <Label text="Password (required):" textFill="WHITE">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font></Label>
            <TextField fx:id="createPasswordInput" />
            <Label text="Re-type Password:" textFill="WHITE">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font></Label>
            <TextField fx:id="reTypePasswordInput" />
            <Label fx:id="createUserErrorMessage">
               <font>
                  <Font size="10.0" />
               </font></Label>
         </children>
      </VBox>
      <Label alignment="CENTER" layoutX="43.0" layoutY="150.0" prefHeight="36.0" prefWidth="263.0" text="Create Your Account" textAlignment="JUSTIFY" textFill="WHITE">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Label>
      <HBox alignment="CENTER" layoutX="75.0" layoutY="348.0" prefHeight="36.0" prefWidth="200.0" spacing="5.0">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackButton" text="BACK" />
            <Button fx:id="createUserButton" mnemonicParsing="false" onAction="#handleCreateNewUserButton" text="CREATE USER" />
         </children>
      </HBox>
      <ImageView fx:id="imageView2" fitHeight="125.0" fitWidth="350.0" nodeOrientation="INHERIT" pickOnBounds="true" />
      <Label layoutX="108.0" layoutY="86.0" text="Course Review">
         <font>
            <Font name="Elephant" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
